COLUMN TABLE "Cars.Generation"(
  "GENERATIONID" INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL COMMENT 'Generation ID',
  "MODELID" INTEGER COMMENT 'Model ID',
  "YEAR_FROM" NUMERIC(4),
  "YEAR_TO" NUMERIC(4),
  PRIMARY KEY ("GENERATIONID"))
  COMMENT 'Cars Generation Table'
  WITH ASSOCIATIONS( JOIN "Cars.Model" AS "Model" ON "Model"."MODELID" = "MODELID") 
COLUMN TABLE "Cars.Model"(
  "MODELID" INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL COMMENT 'Model ID',
  "NAME" NVARCHAR(100),
  "BRANDID" INTEGER COMMENT 'Brand ID',
  PRIMARY KEY ("MODELID"))
  COMMENT 'Cars Model Table'
  WITH ASSOCIATIONS( JOIN "Cars.Brand" AS "Brand" ON "Brand"."BRANDID" = "BRANDID") 
COLUMN TABLE "Cars.Engine"(
  "ENGINEID" INTEGER GENERATED BY DEFAULT AS IDENTITY (START WITH 1 INCREMENT BY 1) NOT NULL COMMENT 'Engine ID',
  "NAME" NVARCHAR(100),
  "DISPLACEMENT" NUMERIC(5),
  "POWER" NUMERIC(4),
  "TORQUE" NUMERIC(4),
  "TYPE" NVARCHAR(2),
  PRIMARY KEY ("ENGINEID")
) COMMENT 'Cars Engine Table'